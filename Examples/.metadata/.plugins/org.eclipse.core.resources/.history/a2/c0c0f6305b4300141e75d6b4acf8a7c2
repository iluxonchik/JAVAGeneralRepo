
public class AlivePreyState implements PreyState{
	
	Prey prey;
	
	public AlivePreyState(Prey prey){
		this.prey = prey;
	}
	
	// Overrides
	public void run() {
		prey.setEnergy(prey.getEnergy() - prey.RUN_ENERGY);
	}

	public void escape() {
		// TODO Auto-generated method stub
		prey.energy = 0;
	}
	
}
