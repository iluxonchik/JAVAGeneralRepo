
public class Dog extends Animal{
	
	public final int INIT_ENERGY = 100;
	public final int RUN_ENERGY = 50;
	
	public Dog(String name){
		this.name = name;
		
		AliveAnimal = new AliveAnimalState((Object)this);
		DeadAninam = new DeadAnimalState((Object)this);
	}
	
	@Override
	public void run() {
		state.run();
	}

	@Override
	public void sleep() {
		state.run();		
	}

	@Override
	public void attack(Prey p) {
		state.run();
	}

}
