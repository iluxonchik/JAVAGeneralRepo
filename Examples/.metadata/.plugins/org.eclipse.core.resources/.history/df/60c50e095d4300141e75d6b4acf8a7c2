
public class Bird extends Prey{

	public Bird(){
		this.energy = INIT_ENERGY;
		AlivePrey= new AlivePreyState(this);
		if (this.getEnergy() >= 0){
			this.state = AliveState;
		}
	}

	@Override
	public void run() {
		state.run();
	}

	@Override
	public void sleep() {
		// TODO Auto-generated method stub
		
	}

}
